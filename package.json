{
  "name": "notes-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "db:studio": "npx drizzle-kit studio --port 3000 --verbose",
    "db:generate": "drizzle-kit generate:pg --config drizzle.config.ts",
