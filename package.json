    "@tailwindcss/forms": "^0.5.6",
    "@types/node": "20.9.0",
    "autoprefixer": "10.4.16",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "postcss": "8.4.31",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "3.3.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vite-plugin-tailwind-purgecss": "0.1.3"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/adapter-postgresql": "^2.0.2",
